        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <mapper namespace="com.example.demo.dao.ManageDao">
            <update id="assignDorm" parameterType="com.example.demo.entity.User">
                update User set dormId=#{dormId} where uid=#{uid}
            </update>
        </mapper>